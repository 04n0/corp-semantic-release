{
  "lines": 90,
  "functions": 85,
  "branches": 80,
  "statements": 90,
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "spec/**/*.spec.js"
  ],
  "reporter": [
    "lcov",
    "text-summary",
    "cobertura",
    "html",
    "json"
  ],
  "report-dir": "reports/coverage",
  "all": true
}
